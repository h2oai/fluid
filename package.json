{
  "name": "fluid",
  "version": "0.0.1",
  "description": "Rapid Application Development for a more... civilized age.",
  "main": "index.js",
  "scripts": {
    "start": "coffee --watch --compile *.coffee & \njade --pretty --watch index.jade & \nbrowser-sync start --server --files \"*.js,*.css,*.html\"",
    "pack": "coffee pack.coffee",
    "test": "coffee --compile *.coffee && node tests.js | faucet",
    "coverage": "coffee --compile *.coffee && istanbul cover tests.js",
    "coveralls": "coffee --compile *.coffee && istanbul cover test.js --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/h2oai/fluid.git"
  },
  "keywords": [
    "material-design",
    "FRP",
    "reactive",
    "dataflow",
    "rapid-application-development"
  ],
  "author": "Prithvi Prabhu <prithvi@h2o.ai> (http://h2o.ai/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/h2oai/fluid/issues"
  },
  "homepage": "https://github.com/h2oai/fluid",
  "devDependencies": {
    "bower": "^1.4.1",
    "browser-sync": "^2.8.0",
    "coveralls": "^2.11.3",
    "faucet": "0.0.1",
    "istanbul": "^0.3.17",
    "tape": "^4.0.2"
  },
  "dependencies": {
    "coffee-script": "^1.9.3",
    "diecut": "0.0.1",
    "jade": "^1.11.0",
    "jss": "^2.1.3",
    "knockout": "^3.3.0",
    "lodash": "^3.10.0",
    "marked": "^0.3.5"
  }
}
